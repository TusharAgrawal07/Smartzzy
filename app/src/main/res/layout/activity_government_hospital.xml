<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#38960096"
    tools:context=".ImportantAddress.government_hospital">

    <TextView
        android:id="@+id/government_hospital_text1"
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:textSize="26sp"
        android:padding="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04" />

    <TextView
        android:id="@+id/government_hospital_text2"
        android:layout_width="293dp"
        android:layout_height="228dp"
        android:fontFamily="@font/cousine"
        android:textSize="18sp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.096"
        android:layout_gravity="center_horizontal" />

    <TextView
        android:id="@+id/government_hospital_text3"
        android:layout_width="76dp"
        android:layout_height="48dp"
        android:fontFamily="@font/cousine"
        android:gravity="center"
        android:text="@string/hyperlink"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.176"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/government_hospital_text2"
        app:layout_constraintVertical_bias="0.149" />

    <TextView
        android:id="@+id/government_hospital_text4"
        android:layout_width="123dp"
        android:layout_height="52dp"
        android:fontFamily="@font/cousine"
        android:gravity="center"
        android:text="@string/hyperlink2"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.614"
        app:layout_constraintStart_toEndOf="@+id/government_hospital_text3"
        app:layout_constraintTop_toBottomOf="@+id/government_hospital_text2"
        app:layout_constraintVertical_bias="0.152" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="263dp"
        android:layout_height="147dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/government_hospital_text1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/diu_government_hospital" />

</androidx.constraintlayout.widget.ConstraintLayout>