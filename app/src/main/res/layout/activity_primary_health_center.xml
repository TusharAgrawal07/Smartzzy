<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_sh"
    tools:context=".ImportantAddress.primary_health_center">

    <TextView
        android:id="@+id/primary_health_center_text4"
        android:layout_width="wrap_content"
        android:layout_height="52dp"
        android:fontFamily="@font/cousine"
        android:gravity="center"
        android:text="@string/hyperlink8"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.576"
        app:layout_constraintStart_toEndOf="@+id/primary_health_center_text3"
        app:layout_constraintTop_toBottomOf="@+id/primary_health_center_text2"
        app:layout_constraintVertical_bias="0.134" />

    <TextView
        android:id="@+id/primary_health_center_text3"
        android:layout_width="76dp"
        android:layout_height="48dp"
        android:fontFamily="@font/cousine"
        android:gravity="center"
        android:text="@string/hyperlink7"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.185"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/primary_health_center_text2"
        app:layout_constraintVertical_bias="0.131" />

    <TextView
        android:id="@+id/primary_health_center_text2"
        android:layout_width="262dp"
        android:layout_height="239dp"
        android:fontFamily="@font/cousine"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.086" />

    <TextView
        android:id="@+id/primary_health_center_text1"
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:textSize="26sp"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="148dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/primary_health_center_text1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/diu_primary_health_center" />
</androidx.constraintlayout.widget.ConstraintLayout>