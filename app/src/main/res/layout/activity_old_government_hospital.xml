<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_sh"
    tools:context=".ImportantAddress.old_government_hospital">

    <TextView
        android:id="@+id/old_government_hospital_text2"
        android:layout_width="283dp"
        android:layout_height="214dp"
        android:fontFamily="@font/cousine"
        android:textSize="18sp"
        android:paddingStart="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.091"
        android:layout_gravity="center_horizontal"
        android:paddingLeft="2dp" />

    <TextView
        android:id="@+id/old_government_hospital_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="72dp"
        android:layout_marginRight="72dp"
        android:fontFamily="@font/cousine"
        android:gravity="center"
        android:text="@string/hyperlink4"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/old_government_hospital_text3"
        app:layout_constraintTop_toBottomOf="@+id/old_government_hospital_text2"
        app:layout_constraintVertical_bias="0.232" />

    <TextView
        android:id="@+id/old_government_hospital_text3"
        android:layout_width="76dp"
        android:layout_height="48dp"
        android:fontFamily="@font/cousine"
        android:gravity="center"
        android:text="@string/hyperlink3"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.191"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/old_government_hospital_text2"
        app:layout_constraintVertical_bias="0.224" />

    <TextView
        android:id="@+id/old_government_hospital_text1"
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:textSize="26sp"
        android:padding="2dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.04"
        android:layout_gravity="center_horizontal" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="261dp"
        android:layout_height="153dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/old_government_hospital_text1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/diu_old_government_hospital" />

</androidx.constraintlayout.widget.ConstraintLayout>